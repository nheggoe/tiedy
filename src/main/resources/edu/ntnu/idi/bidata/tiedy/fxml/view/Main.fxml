<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>
<BorderPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: fff8dc;"
            xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="edu.ntnu.idi.bidata.tiedy.frontend.controller.MainController">
    <top>
        <fx:include fx:id="menuBar" source="../component/MenuBar.fxml"/>
    </top>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <center>
        <AnchorPane BorderPane.alignment="CENTER">
            <children>
                <FlowPane fx:id="flowPane" alignment="CENTER" AnchorPane.bottomAnchor="0.0"
                          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                          AnchorPane.topAnchor="0.0">
                    <children>
                        <Label contentDisplay="CENTER">
                            <font>
                                <Font size="53.0"/>
                            </font>
                        </Label>
                    </children>
                </FlowPane>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
