<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #9A9696;"
            xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="edu.ntnu.idi.bidata.tiedy.frontend.controller.LoginController">
    <children>
        <AnchorPane layoutX="132.0" layoutY="97.0" prefHeight="206.0" prefWidth="336.0">
            <children>
                <PasswordField id="passwordField" fx:id="passwordField" layoutX="136.0" layoutY="106.0"
                               prefHeight="33.0" prefWidth="160.0" style="-fx-background-color: #D9D9D9;"/>
                <TextField id="usernameField" fx:id="usernameField" layoutX="136.0" layoutY="63.0" prefHeight="33.0"
                           prefWidth="160.0" style="-fx-background-color: #D9D9D9;"/>
                <Label layoutX="42.0" layoutY="63.0" style="-fx-background-color: #D9D9D9;" text="username">
                    <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </padding>
                </Label>
                <Label layoutX="42.0" layoutY="106.0" style="-fx-background-color: #D9D9D9;" text="password">
                    <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </padding>
                </Label>
                <Button id="loginButton" layoutX="144.0" layoutY="167.0" mnemonicParsing="false" onAction="#loginUser"
                        text="Login"/>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
